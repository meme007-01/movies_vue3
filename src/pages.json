{
  "easycom": {
    "custom": {
      "^u--(.*)": "uview-plus/components/u-$1/u-$1.vue",
      "^up-(.*)": "uview-plus/components/u-$1/u-$1.vue",
      "^u-([^-].*)": "uview-plus/components/u-$1/u-$1.vue",
      "^(?!z-paging-refresh|z-paging-load-more)z-paging(.*)": "z-paging/components/z-paging$1/z-paging$1.vue"
    }
  },
  "pages": [{
      "path": "pages/tab/home/index",
      "style": {
        "navigationBarTitleText": "首页",
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/tab/list/index",
      "style": {
        "navigationBarTitleText": "列表"
      }
    },
    {
      "path": "pages/tab/user/index",
      "style": {
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/tab/hot/index",
      "style": {
        "navigationBarTitleText": "",
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/tab/find/index",
      "style": {
        "navigationBarTitleText": ""
      }
    },
    {
      "path": "pages/tab/vip/index",
      "style": {
        "navigationBarTitleText": "",
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/video_detail/index",
      "style": {
        "navigationBarTitleText": "",
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/tab/my/index",
      "style": {
        "navigationBarTitleText": "",
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/login/index",
      "style": {
        "navigationBarTitleText": "",
        "navigationStyle": "custom"
      }
    }
  ],
  "subPackages": [{
    "root": "pages/common",
    "pages": [{
        "path": "login/index",
        "navigationStyle": "custom"
      },
      {
        "path": "webview/index",
        "navigationBarTitleText": "网页"
      }
    ]
  }],
  "preloadRule": {
    "pages/tab/home/index": {
      "network": "all",
      "packages": ["pages/common"]
    }
  },
  "tabBar": {
    "color": "#7A7E83",
    "selectedColor": "#FFF",
    "borderStyle": "#101012",
    "backgroundColor": "#101012",
    "list": [{
      "pagePath": "pages/tab/home/index",
      "selectedIconPath": "static/tabbar/home_active.png",
      "iconPath": "static/tabbar/home.png",
      "text": "首页"
    }, {
      //   "pagePath": "pages/tab/find/index",
      //   "selectedIconPath": "static/tabbar/find_active.png",
      //   "iconPath": "static/tabbar/find.png",
      //   "text": "发现"
      // }, {
      "pagePath": "pages/tab/vip/index",
      "selectedIconPath": "static/tabbar/vip_active.png",
      "iconPath": "static/tabbar/vip.png",
      "text": "VIP"
    }, {
      "pagePath": "pages/tab/hot/index",
      "selectedIconPath": "static/tabbar/hot_active.png",
      "iconPath": "static/tabbar/hot.png",
      "text": "热门"
    }, {
      "pagePath": "pages/tab/my/index",
      "selectedIconPath": "static/tabbar/my_active.png",
      "iconPath": "static/tabbar/my.png",
      "text": "我的"
    }]
  },
  "globalStyle": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "uni-app",
    "navigationBarBackgroundColor": "#F8F8F8",
    "backgroundColor": "#F8F8F8"
  }
}
